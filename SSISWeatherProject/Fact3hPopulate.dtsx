      DTS:ObjectName="Populate Fact 3H table"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{29BE8F39-EBB7-4315-9621-D049770CEE09}"
          SQLTask:SqlStatementSource="INSERT INTO [dbo].FactWeather3h (&#xA;&#x9;[WeatherStationKey],&#xA;&#x9;[Date],&#xA;&#x9;[TimeInterval],&#xA;&#x9;[ForecastMeanMaxTemp_degC],&#xA;&#x9;[CurrentMeanMaxTemp_degC], &#xA;&#x9;[ForecastTotalRain_mm], &#xA;&#x9;[CurrentTotalRain_mm] ) &#xA;SELECT [DimWeatherStation].WeatherStationKey, [Current].[Date], [Current].[TimeInterval], [Forecast_MeanMaxTemp_degC], [Current_MeanMaxTemp_degC], [Forecast_TotalRain_mm], [Current_TotalRain_mm]&#xA;FROM&#xA;   (SELECT [StationId],&#xA;&#x9;&#x9;CAST(MeasurementDate as date) [Date], &#xA;&#x9;&#x9;MIN((( DATEPART(hour, [MeasurementDate])*60 + DATEPART(minute, [MeasurementDate]))/180) + 1) as [TimeInterval],&#xA;&#x9;&#x9;AVG(MaxTemp) as [Current_MeanMaxTemp_degC], &#xA;&#x9;&#x9;AVG(ISNULL(Rain,0)) as [Current_TotalRain_mm]&#x9;&#x9;&#xA;&#x9;FROM [dbo].[StagingWeatherCurrent]&#xA;&#x9;WHERE [LoadedStamp] &gt;?&#xA;&#x9;-- there should not be more than 1 record if current weather load is executed once every 3 hours (groupping just in case)&#xA;&#x9;GROUP BY [StationId], cast([MeasurementDate] as date), ((( DATEPART(hour, [MeasurementDate])*60 + DATEPART(minute, [MeasurementDate]))/180) + 1)&#xA;&#x9;) AS [Current]&#xA;&#x9;INNER JOIN&#xA;&#x9;(SELECT [StationId],&#xA;&#x9;&#x9;CAST([MeasurementDate] as date) [Date], &#xA;&#x9;&#x9;(( DATEPART(hour, [MeasurementDate])*60 + DATEPART(minute, [MeasurementDate]))/180) + 1 as [TimeInterval],&#xA;&#x9;&#x9;[MaxTemp] as [Forecast_MeanMaxTemp_degC], &#xA;&#x9;&#x9;ISNULL([Rain],0) as [Forecast_TotalRain_mm]&#xA;&#x9;FROM [dbo].[StagingWeatherForecast]&#xA;&#x9;) AS Forecast&#xA;&#x9;ON [Current].StationId = Forecast.StationId AND [Current].[date] = Forecast.[date] AND [Current].[TimeInterval] = Forecast.[TimeInterval]&#xA;&#x9;INNER JOIN &#x9;[dbo].[DimWeatherStation] ON [Current].StationId = [DimWeatherStation].StationId" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="0"
            SQLTask:DtsVariableName="User::LastExecutionDateTime"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="7"
            SQLTask:ParameterSize="-1" />
        </SQLTask:SqlTaskData>
